<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/common_color_C2C2C2">
    <item android:id="@android:id/mask">
        <shape android:shape="oval">
            <solid android:color="?attr/common_background_color" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <size
                android:width="10dp"
                android:height="10dp" />
            <solid android:color="?attr/common_text_secondary_color" />
            <stroke
                android:width="1dp"
                android:color="?attr/common_background_color"
                android:dashWidth="1dp" />
        </shape>
    </item>
</ripple>
